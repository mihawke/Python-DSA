   "source": [
    "<h2>Lowest Common Multiple</h2>\n",
    "<ul>\n",
    "<li>LCM = (m*n)/GCF</li>\n",
    "</ul>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def LCM(m,n):\n",
    "    multiple = m*n\n",
    "    # m > n\n",
    "    if n > m:\n",
    "        (m,n)=(n,m)\n",
    "    while m%n != 0:\n",
    "        (m,n)=(n,m%n)\n",
    "    return multiple/n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
